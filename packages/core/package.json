{
  "name": "@nura/core",
  "version": "0.1.0",
  "description": "Core primitives for building AI-friendly interfaces with Nura.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-dev/nura.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/nura-dev/nura/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/nura-dev/nura/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    },
    "./entities": {
      "import": "./dist/entities.js",
      "require": "./dist/entities.cjs",
      "types": "./dist/entities.d.ts"
    },
    "./i18n": {
      "import": "./dist/i18n.js",
      "require": "./dist/i18n.cjs",
      "types": "./dist/i18n.d.ts"
    },
    "./lexicon": {
      "import": "./dist/lexicon.js",
      "require": "./dist/lexicon.cjs",
      "types": "./dist/lexicon.d.ts"
    },
    "./registry": {
      "import": "./dist/registry.js",
      "require": "./dist/registry.cjs",
      "types": "./dist/registry.d.ts"
    },
    "./nlp/numerals": {
      "import": "./dist/nlp/numerals.js",
      "require": "./dist/nlp/numerals.cjs",
      "types": "./dist/nlp/numerals.d.ts"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "sideEffects": false,
  "typesVersions": {
    "*": {
      "entities": ["types/entities.d.ts"],
      "i18n": ["types/i18n.d.ts"],
      "lexicon": ["types/lexicon.d.ts"],
      "registry": ["types/registry.d.ts"],
      "nlp/*": ["types/nlp/*"]
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/entities.ts src/i18n.ts src/lexicon.ts src/registry.ts src/nlp/numerals.ts --dts --format esm,cjs --treeshake --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "nura",
    "ai",
    "semantic",
    "commands"
  ],
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@nura/plugin-fuzzy": "workspace:*",
    "tslib": "^2.6.2"
  }
}
