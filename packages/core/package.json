{
  "name": "@nura/core",
  "version": "0.1.0",
  "description": "Core primitives for building AI-friendly interfaces with Nura.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-dev/nura.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/nura-dev/nura/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/nura-dev/nura/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": ["dist"],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint . --ext .ts,.tsx,.mts,.cts",
    "lint:fix": "eslint . --ext .ts,.tsx,.mts,.cts --fix",
    "test": "pnpm build && node --test \"test/**/*.mjs\"",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "nura",
    "ai",
    "semantic",
    "commands"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@nura/plugin-fuzzy": "workspace:*",
    "tslib": "^2.6.2"
  }
}
