{
  "name": "@nura/core",
  "version": "0.1.0",
  "description": "Core primitives for building AI-friendly interfaces with Nura.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-dev/nura.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/nura-dev/nura/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/nura-dev/nura/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "import": "./dist/i18n.js"
    },
    "./lexicon": {
      "types": "./dist/lexicon.d.ts",
      "import": "./dist/lexicon.js"
    },
    "./entities": {
      "types": "./dist/entities.d.ts",
      "import": "./dist/entities.js"
    },
    "./seeds/lexicon": {
      "types": "./dist/seeds/lexicon.d.ts",
      "import": "./dist/seeds/lexicon.js"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "import": "./dist/registry.js"
    },
    "./nlp/numerals": {
      "types": "./dist/nlp/numerals.d.ts",
      "import": "./dist/nlp/numerals.js"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "import": "./dist/actions.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "": ["dist/index.d.ts"],
      "i18n": ["dist/i18n.d.ts"],
      "lexicon": ["dist/lexicon.d.ts"],
      "entities": ["dist/entities.d.ts"],
      "seeds/lexicon": ["dist/seeds/lexicon.d.ts"],
      "registry": ["dist/registry.d.ts"],
      "nlp/numerals": ["dist/nlp/numerals.d.ts"],
      "actions": ["dist/actions.d.ts"]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "lint": "eslint . --ext .ts,.tsx,.mts,.cts",
    "lint:fix": "eslint . --ext .ts,.tsx,.mts,.cts --fix",
    "test": "pnpm build && node --test \"test/**/*.mjs\"",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "nura",
    "ai",
    "semantic",
    "commands"
  ],
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "rollup": "^4.24.0",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@nura/plugin-fuzzy": "workspace:*",
    "tslib": "^2.6.2"
  }
}
