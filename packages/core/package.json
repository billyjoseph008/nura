{
  "name": "@nura/core",
  "version": "0.1.0",
  "description": "Core primitives for building AI-friendly interfaces with Nura.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-dev/nura.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/nura-dev/nura/tree/main/packages/core",
  "bugs": {
    "url": "https://github.com/nura-dev/nura/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "import": "./dist/i18n.js"
    },
    "./lexicon": {
      "types": "./dist/lexicon.d.ts",
      "import": "./dist/lexicon.js"
    },
    "./entities": {
      "types": "./dist/entities.d.ts",
      "import": "./dist/entities.js"
    },
    "./seeds/lexicon": {
      "types": "./dist/seeds/lexicon.d.ts",
      "import": "./dist/seeds/lexicon.js"
    },
    "./registry": {
      "types": "./dist/registry.d.ts",
      "import": "./dist/registry.js"
    },
    "./nlp/numerals": {
      "types": "./dist/nlp/numerals.d.ts",
      "import": "./dist/nlp/numerals.js"
    },
    "./actions": {
      "types": "./dist/actions.d.ts",
      "import": "./dist/actions.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "": ["dist/index.d.ts"],
      "i18n": ["dist/i18n.d.ts"],
      "lexicon": ["dist/lexicon.d.ts"],
      "entities": ["dist/entities.d.ts"],
      "seeds/lexicon": ["dist/seeds/lexicon.d.ts"],
      "registry": ["dist/registry.d.ts"],
      "nlp/numerals": ["dist/nlp/numerals.d.ts"],
      "actions": ["dist/actions.d.ts"]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts src/i18n.ts src/lexicon.ts src/entities.ts src/seeds/lexicon.ts src/registry.ts src/actions.ts src/nlp/numerals.ts --dts --format esm --clean",
    "dev": "tsup src/index.ts src/i18n.ts src/lexicon.ts src/entities.ts src/seeds/lexicon.ts src/registry.ts src/actions.ts src/nlp/numerals.ts --dts --format esm --watch",
    "lint": "eslint . --ext .ts,.tsx,.vue --max-warnings=0",
    "test": "echo \"(todo)\""
  },
  "keywords": [
    "nura",
    "ai",
    "semantic",
    "commands"
  ],
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "@nura/plugin-fuzzy": "workspace:*"
  }
}
