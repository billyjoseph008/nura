{
  "name": "@nura/vue",
  "version": "0.1.0",
  "description": "Vue adapter for Nura.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-dev/nura.git",
    "directory": "packages/vue"
  },
  "homepage": "https://github.com/nura-dev/nura/tree/main/packages/vue",
  "bugs": {
    "url": "https://github.com/nura-dev/nura/issues"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": ["dist", "README.md", "LICENSE"],
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm,cjs --treeshake --clean",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": ["nura", "vue", "directives", "composables"],
  "dependencies": {
    "@nura/core": "workspace:*"
  },
  "peerDependencies": {
    "vue": ">=3.3.0",
    "@nura/core": ">=0.1.0"
  },
  "devDependencies": {
    "tsup": "^8.5.0",
    "typescript": "^5.4.0"
  }
}
