{
  "name": "@nura/svelte",
  "version": "0.1.0",
  "description": "Svelte adapter for Nura.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-dev/nura.git",
    "directory": "packages/svelte"
  },
  "homepage": "https://github.com/nura-dev/nura/tree/main/packages/svelte",
  "bugs": {
    "url": "https://github.com/nura-dev/nura/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "echo \"(todo)\"",
    "dev": "echo \"(todo)\"",
    "lint": "eslint . --ext .ts,.tsx,.mts,.cts",
    "lint:fix": "eslint . --ext .ts,.tsx,.mts,.cts --fix",
    "clean": "rm -rf dist",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "nura",
    "svelte",
    "actions",
    "stores"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0 || ^5.0.0"
  },
  "dependencies": {
    "@nura/core": "workspace:*",
    "@nura/dom": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "svelte": "^4.2.0",
    "tsup": "^8.0.0",
    "typescript": "^5.5.0"
  }
}
