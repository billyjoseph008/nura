{
  "name": "@nura/plugin-fuzzy",
  "version": "0.1.0",
  "description": "Lightweight fuzzy matching utilities for Nura.js",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-dev/nura.git",
    "directory": "packages/plugin-fuzzy"
  },
  "homepage": "https://github.com/nura-dev/nura/tree/main/packages/plugin-fuzzy",
  "bugs": {
    "url": "https://github.com/nura-dev/nura/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsup src/index.ts --dts --format esm --clean",
    "dev": "tsup src/index.ts --dts --format esm --watch",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "test": "pnpm build && node --test \"test/**/*.mjs\""
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.4.0"
  }
}
