{
  "name": "nura",
  "version": "0.1.0",
  "private": true,
  "description": "Make your app breathe - AI-friendly web applications",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nura-dev/nura.git"
  },
  "homepage": "https://github.com/nura-dev/nura",
  "bugs": {
    "url": "https://github.com/nura-dev/nura/issues"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "pnpm -r --filter \"!./apps/**\" --filter \"!./packages/examples/**\" run build",
    "typecheck": "pnpm -r --filter \"@nura/*\" --filter \"!@nura/examples\" run typecheck",
    "build:all": "pnpm -r run build",
    "typecheck:all": "pnpm -r run typecheck",
    "lint": "eslint .",
    "format:check": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@nura/core": "workspace:*",
    "@nura/dom": "workspace:*",
    "@nura/react": "workspace:*",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "react": "^19",
    "react-dom": "^19",
    "svelte": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "latest",
    "vue": "latest"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.8",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@tailwindcss/postcss": "^4.1.14",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-import-resolver-typescript": "^3.10.1",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsdoc": "^48.10.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-vue": "^10.5.1",
    "fuse.js": "^7.0.0",
    "gh-pages": "^6.1.1",
    "postcss": "^8.5",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.7",
    "tailwindcss": "4.1.14",
    "turbo": "^2.0.0",
    "typedoc": "^0.25.13",
    "typescript": "^5",
    "vitepress": "^1.4.3",
    "vitepress-plugin-search": "1.0.4-alpha.22",
    "vue-eslint-parser": "^10.2.0"
  },
  "packageManager": "pnpm@8.15.0",
  "engines": {
    "node": ">=18.18.0",
    "pnpm": ">=8.0.0"
  }
}
